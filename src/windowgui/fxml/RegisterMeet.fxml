<AnchorPane fx:id="rootPane" onMouseClicked="#resetIt" prefHeight="463.0" prefWidth="393.0" style="-fx-background-color: green;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="windowgui.controller.RegisterMeetController">
	<children>
