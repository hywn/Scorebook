      <Label layoutX="14.0" layoutY="198.0" text="Temperature: ">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="addressField" layoutX="220.0" layoutY="141.0" onInputMethodTextChanged="#reset" />
      <TextField fx:id="tempField" layoutX="224.0" layoutY="201.0" onInputMethodTextChanged="#reset" prefHeight="25.0" prefWidth="142.0" />
      <ComboBox fx:id="indoorsOptions" layoutX="219.0" layoutY="171.0" onAction="#reset" prefWidth="150.0" />
      <Label fx:id="doneLabel" layoutX="14.0" layoutY="298.0" text="Meet has been registered successfully!" textFill="WHITE" visible="false">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
	</children>
</AnchorPane>
