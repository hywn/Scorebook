<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.*?>


<AnchorPane fx:id="rootPane"
	fx:controller="windowgui.controller.ViewDataController" prefWidth="600"
	prefHeight="500" style="-fx-background-color: green;"
	xmlns:fx="http://javafx.com/fxml">
	<children>
		<TableView fx:id="scoreDataTable" prefWidth="500"
			prefHeight="400" layoutX="50" layoutY="100">
		</TableView>
		<fx:include source="HomeButton.fxml"
			onMouseClicked="#goHome" />
		<Label layoutX="100" layoutY="50" text="DATA">
			<font>
				<Font name="Avenir Heavy" size="24.0" />
			</font>
		</Label>
		<ImageView fitHeight="46.0" fitWidth="61.0" layoutX="200.0"
			onMouseClicked="#goBack" layoutY="47.0" pickOnBounds="true"
			preserveRatio="true">
			<image>
				<Image url="@../back.png" />
			</image>
		</ImageView>
	</children>
</AnchorPane>