         <Button layoutX="342.0" layoutY="124.0" mnemonicParsing="false" onAction="#viewData" prefHeight="75.0" prefWidth="150.0" style="-fx-background-radius: 10;" text="View past data" />
         <Button layoutX="26.0" layoutY="217.0" mnemonicParsing="false" onAction="#aboutPage" prefHeight="75.0" prefWidth="150.0" style="-fx-background-radius: 10;" text="About" />
         <Button layoutX="342.0" layoutY="217.0" mnemonicParsing="false" onAction="#exit" prefHeight="75.0" prefWidth="150.0" style="-fx-background-radius: 10;" text="Exit" />
      </children>
   </Pane>
</AnchorPane>
