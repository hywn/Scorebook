<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="rootPane" prefHeight="600" prefWidth="600.0" stylesheets="windowgui/Style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="windowgui.controller.MainMenuController">
   <ImageView>
      <image>
         <Image url="@../MainMenu.png" />
      </image>
   </ImageView>
   <Pane layoutX="40.0" layoutY="106.0" prefHeight="389.0" prefWidth="520.0">
      <children>
         <Button layoutX="342.0" layoutY="24.0" mnemonicParsing="false" onAction="#registerAthlete" prefHeight="75.0" prefWidth="150.0" style="-fx-background-radius: 10;" text="Register a new Athlete" textAlignment="CENTER" wrapText="true" />
         <Button layoutX="26.0" layoutY="24.0" mnemonicParsing="false" onAction="#inputData" prefHeight="75.0" prefWidth="150.0" style="-fx-background-radius: 10;" text="Record Data" />
