			<font>
				<Font size="20.0" />
			</font>
		</Label>
		<fx:include onMouseClicked="#goHome" source="HomeButton.fxml" />
		<ComboBox fx:id="roundOptions" layoutX="279.0" layoutY="78.0" onAction="#reset" prefHeight="25.0" prefWidth="96.0" />
		<Button layoutX="303.0" layoutY="328.0" mnemonicParsing="false" onAction="#enter" prefHeight="30.0" prefWidth="81.0" style="-fx-background-radius: 15;" text="Enter" />
		<Label layoutX="14.0" layoutY="102.0" prefHeight="36.0" prefWidth="238.0" text="Meet number: ">
         <font>
            <Font size="19.0" />
         </font></Label>
      <TextField fx:id="meetNumField" layoutX="252.0" layoutY="108.0" onInputMethodTextChanged="#reset" prefHeight="25.0" prefWidth="117.0" />
      <Label layoutX="14.0" layoutY="138.0" text="Meet location: ">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="168.0" text="Is the meet indoors?">
